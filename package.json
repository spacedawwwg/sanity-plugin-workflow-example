{
  "name": "sanity-plugin-workflow-spacedawwwg",
  "version": "1.0.6",
  "description": "A demonstration of a custom content publishing workflow using Sanity.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/spacedawwwg/sanity-plugin-workflow#readme",
  "bugs": {
    "url": "https://github.com/spacedawwwg/sanity-plugin-workflow/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-workflow.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf lib",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.6.0",
    "@sanity/icons": "^2.11.8",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@tanstack/react-virtual": "^3.2.0",
    "framer-motion": "^11.0.28",
    "groq": "^3.37.2",
    "lexorank": "^1.0.5",
    "react-fast-compare": "^3.2.2",
    "sanity-plugin-utils": "^1.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@sanity/pkg-utils": "^6.4.1",
    "@sanity/plugin-kit": "^3.1.12",
    "@sanity/semantic-release-preset": "^4.1.7",
    "@types/react": "^18.2.75",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.37.2",
    "semantic-release": "^23.0.8",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@sanity/ui": "^1.0 || ^2.0",
    "react": "^18",
    "react-dom": "^18",
    "sanity": "^3.0.0",
    "styled-components": "^5.0 || ^6.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  },
  "browserslist": "extends @sanity/browserslist-config",
  "sideEffects": true
}
